{
  "manifest_version": 3,
  "name": "Twitter Bookmarks Gallery",
  "version": "1.0",
  "description": "View your Twitter bookmarks as an image gallery",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "https://twitter.com/*",
    "https://x.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Twitter Bookmarks Gallery"
  }
}